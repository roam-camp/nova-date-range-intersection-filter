{
    "name": "roam-camp/nova-date-range-intersection-filter",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "filter"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RoamCamp\\Filters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RoamCamp\\Filters\\FilterServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
